<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/homes"
    tools:context=".landingpage">

    <Button
        android:id="@+id/signinC"
        android:layout_width="250dp"
        android:layout_height="55dp"
        android:layout_marginTop="364dp"
        android:layout_x="92dp"
        android:layout_y="325dp"
        android:backgroundTint="#128C80"
        android:fontFamily="@font/poppins_regular"
        android:text="SIGN UP"
        android:textSize="15sp"
        android:layout_centerHorizontal="true"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/t111"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/signinC"
        android:layout_marginStart="80dp"
        android:layout_x="91dp"
        android:layout_y="389dp"
        android:fontFamily="@font/poppins_regular"
        android:text="Already have an Account?"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/signinC"
        app:layout_constraintVertical_bias="0.01" />

    <EditText
        android:id="@+id/landinganchor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="287dp"
        android:layout_y="389dp"
        android:background="?android:selectableItemBackground"
        android:layout_marginStart="5dp"
        android:clickable="true"
        android:focusable="false"
        android:layout_toRightOf="@+id/t111"
        android:fontFamily="@font/poppins_regular"
        android:text="Click me"
        android:textColor="#128C80"
        android:textSize="15dp"

        android:layout_below="@+id/signinC"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/t111"
        app:layout_constraintTop_toBottomOf="@+id/signinC"
        app:layout_constraintVertical_bias="0.01" />

</RelativeLayout>
